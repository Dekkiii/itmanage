<script>
  const salestype = {
    labels: ['1 Month', '3 Months', '6 Months', '12 Months'],
    datasets: [{
      label: 'Sales Type',
      data: [<%= mon1 %>, <%= mon3 %>, <%= mon6 %>, <%= mon12 %>],
      backgroundColor: [
        'rgba(54, 162, 235, 0.2)',
        'rgba(255, 99, 132, 0.2)',
        'rgba(75, 192, 192, 0.2)',
        'rgba(92, 15, 192, 0.2)'
      ],
      borderColor: [
        'rgba(54, 162, 235, 1)',
        'rgba(255, 99, 132, 1)',
        'rgba(75, 192, 192, 1)',
        'rgba(92, 15, 192, 1)'
      ],
      borderWidth: 1
    }]
  };
  const salestypeConfig = {
    type: 'doughnut',
    data: salestype,
    options: {
    interaction: {
      mode: 'index', // Only show one tooltip item per dataset
      intersect: false, // Disable hover and click interactions
    },
    plugins: {
      tooltip: {
        enabled: true, // Enable tooltips
        mode: 'index', // Only show one tooltip item per dataset
        intersect: false, // Disable tooltip intersection with the label
      },
    },
  },
  };
  const salestypeCtx = document.getElementById('salesTypeChart').getContext('2d');
  new Chart(salestypeCtx, salestypeConfig);
  </script>

